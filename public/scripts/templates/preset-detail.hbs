<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">Preset {{name}}</h3>
    </div>
    <div class="panel-body">
        <p>Created: {{dateFormat created format="DD-MMM-YYYY"}}</p>
        <p>{{description}}</p>
        <ul class="js-preset-details">
        </ul>
        <a href="#/presets/{{_id}}" class="btn btn-primary pull-right">Edit</a>
    </div>
</div>
